{
    "name": "sackrin/laravel-meta-fields",
    "type": "library",
    "description": "Customisable meta fields for laravel models",
    "keywords": ["laravel","meta-fields"],
    "license": "MIT",
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Hilary Osborne",
            "email": "h.t.osborne@gmail.com",
            "homepage": "https://www.hilaryosborne.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "anahkiasen/underscore-php" : "2.0.0",
        "illuminate/console": "~5.4",
        "illuminate/database": "~5.4",
        "illuminate/http": "~5.4",
        "illuminate/support": "~5.4"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LMF\\LMFServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-0": {
            "LMF": "src"
        }
    }
}
